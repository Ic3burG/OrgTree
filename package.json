{
  "name": "org-directory-tree",
  "version": "1.0.0",
  "description": "Interactive hierarchical organization directory with tree-style navigation",
  "type": "module",
  "license": "GPL-3.0-or-later",
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=9.0.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:all": "npm test && cd server && npm test",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,json,css}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,json,css}\"",
    "lint:all": "npm run lint && cd server && npm run lint",
    "typecheck": "tsc --noEmit",
    "sdk:generate": "openapi-typescript server/src/openapi.yaml -o src/sdk/api-types.ts",
    "prepare": "node -e \"process.exit(process.env.CI ? 0 : 1)\" || husky",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:chromium": "playwright test --project=chromium"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "src/**/*.{json,css}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@sentry/react": "^10.34.0",
    "@simplewebauthn/browser": "^13.2.2",
    "dagre": "^0.8.5",
    "html-to-image": "^1.11.13",
    "jspdf": "^4.0.0",
    "lucide-react": "^0.562.0",
    "papaparse": "^5.4.1",
    "qrcode": "^1.5.4",
    "qrcode.react": "^4.2.0",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-router-dom": "^7.9.6",
    "reactflow": "^11.11.4",
    "socket.io-client": "^4.8.3",
    "xml2js": "^0.6.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@playwright/test": "^1.57.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@types/dagre": "^0.7.53",
    "@types/node": "^25.0.9",
    "@types/papaparse": "^5.5.2",
    "@types/qrcode.react": "^3.0.0",
    "@types/react": "^19.2.8",
    "@types/react-dom": "^19.2.3",
    "@types/xml2js": "^0.4.14",
    "@typescript-eslint/eslint-plugin": "^8.53.0",
    "@typescript-eslint/parser": "^8.52.0",
    "@vitejs/plugin-react": "^5.1.2",
    "@vitest/coverage-v8": "^4.0.17",
    "autoprefixer": "^10.4.16",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "happy-dom": "^20.3.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "openapi-typescript": "^7.10.1",
    "postcss": "^8.4.32",
    "prettier": "^3.8.0",
    "tailwindcss": "^4.1.18",
    "typescript": "^5.9.3",
    "vite": "^7.3.1",
    "vitest": "^4.0.16"
  },
  "optionalDependencies": {
    "@rollup/rollup-darwin-arm64": "^4.55.1",
    "@rollup/rollup-darwin-x64": "^4.55.1"
  }
}
