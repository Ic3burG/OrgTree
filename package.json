{
  "name": "org-directory-tree",
  "version": "1.0.0",
  "description": "Interactive hierarchical organization directory with tree-style navigation",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:all": "npm test && cd server && npm test",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "format": "prettier --write \"src/**/*.{js,jsx,json,css}\"",
    "format:check": "prettier --check \"src/**/*.{js,jsx,json,css}\"",
    "lint:all": "npm run lint && cd server && npm run lint",
    "prepare": "husky"
  },
  "lint-staged": {
    "src/**/*.{js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "src/**/*.{json,css}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@sentry/react": "^10.32.1",
    "@types/dagre": "^0.7.53",
    "dagre": "^0.8.5",
    "html-to-image": "^1.11.13",
    "jspdf": "^3.0.4",
    "lucide-react": "^0.294.0",
    "papaparse": "^5.4.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^7.9.6",
    "reactflow": "^11.11.4",
    "socket.io-client": "^4.8.3",
    "xml2js": "^0.6.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@testing-library/user-event": "^14.6.1",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-v8": "^4.0.16",
    "autoprefixer": "^10.4.16",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "happy-dom": "^20.0.11",
    "husky": "^9.1.7",
    "jsdom": "^27.4.0",
    "lint-staged": "^16.2.7",
    "postcss": "^8.4.32",
    "prettier": "^3.7.4",
    "tailwindcss": "^3.4.0",
    "vite": "^7.2.4",
    "vitest": "^4.0.16"
  },
  "optionalDependencies": {
    "@rollup/rollup-darwin-arm64": "^4.54.0",
    "@rollup/rollup-darwin-x64": "^4.54.0"
  }
}
